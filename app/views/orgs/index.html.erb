<%= render partial: 'layouts/index_top', locals: {items_name: 'orgs', headings: ['Org', 'Slug'], new_item: true, item_name: 'org'} %>

        <% @orgs.each do |org| %>
          <%= render partial: 'layouts/index_row', locals: {
            row: org, 
            columns: [
            {link: org_path(org), text: org.name},
            {text: org.slug}
            ]} %>
        <% end %>

<%= render partial: 'layouts/index_bottom' %>
