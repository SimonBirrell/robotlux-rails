<%= render partial: 'layouts/show_top', locals: {item: @robot_instance, item_class: 'robot_instance', breadcrumb: [
      {link: org_path(@robot_instance.org), text: current_user.org.name},
      {link: robot_instances_path, text: 'Robot Instances'},
      {text: @robot_instance.name}
      ]} %>       

        <%= render partial: 'layouts/show_field', locals: {field_name: 'Name', field_id: 'name', field_value: @robot_instance.name} %>

        <%= render partial: 'layouts/show_field', locals: {field_name: 'Org', field_id: 'org', field_value: @robot_instance.org.name} %>

        <%= render partial: 'layouts/show_field', locals: {field_name: 'Master Key', field_id: 'master-key', field_value: @robot_instance.master_key} %>

        <%= render partial: 'layouts/show_field', locals: {field_name: 'Launch Command', field_id: 'launch-command', field_value: @robot_instance.launch_command} %>

        <%= render partial: 'layouts/show_field', locals: {field_name: 'Network', field_id: 'network', field_value: @robot_instance.network} %>


<%= render 'layouts/show_bottom' %>

<%= render partial: 'layouts/index_top', locals: {items_name: 'robot_instance_sessions', headings: ['Name', 'Robot Instance', 'Start', 'End', 'Token'], index_style: :sub_table} %>

        <% @robot_instance.robot_instance_sessions.each do |robot_instance_session| %>
          <%= render partial: 'layouts/index_row', locals: {
            row: robot_instance_session, 
            columns: [
            {link: robot_instance_session_path(robot_instance_session), text: robot_instance_session.name},
            {link: robot_instance_path(robot_instance_session.robot_instance), text: robot_instance_session.robot_instance.name},
            {text: robot_instance_session.start_session},
            {text: robot_instance_session.end_session},
            {text: robot_instance_session.session_token}
            ]} %>
        <% end %>

<%= render partial: 'layouts/index_bottom' %>
