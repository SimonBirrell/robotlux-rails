
<%= simple_form_for(@robot_instance) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :network %>
    <%= f.input :master_key %>
    <%= f.input :launch_command %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

