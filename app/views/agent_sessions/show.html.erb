<%= render partial: 'layouts/show_top', locals: {item: @agent_session, item_class: 'agent_session', breadcrumb: [
      {link: org_path(@agent_session.agent.org), text: @agent_session.agent.org.name},
      {link: agent_path(@agent_session.agent), text: @agent_session.agent.name},
      {text: @agent_session.start_time}
      ]} %>       

        <%= render partial: 'layouts/show_field', locals: {field_name: 'Start Time', field_id: 'start_time', field_value: @agent_session.start_time} %>

        <%= render partial: 'layouts/show_field', locals: {field_name: 'End Time', field_id: 'end_time', field_value: @agent_session.end_time} %>

        <%= render partial: 'layouts/show_field', locals: {field_name: 'Hostname', field_id: 'hostname', field_value: @agent_session.hostname} %>

        <%= render partial: 'layouts/show_field', locals: {field_name: 'ROS Master URI', field_id: 'ros-master-uri', field_value: @agent_session.ros_master_uri} %>

        <%= render partial: 'layouts/show_field', locals: {field_name: 'Session Status', field_id: 'session-status', field_value: @agent_session.session_status} %>

        <%= render partial: 'layouts/show_field', locals: {field_name: 'Token', field_id: 'token', field_value: @agent_session.token} %>
 
<%= render 'layouts/show_bottom' %>

