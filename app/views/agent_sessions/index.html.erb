<%= render partial: 'layouts/index_top', locals: {items_name: 'agent_sessions', headings: ['Agent', 'Start', 'End', 'Hostname', 'ROS Master URI', 'Status', 'Token']} %>

        <% @agent_sessions.each do |agent_session| %>
          <%= render partial: 'layouts/index_row', locals: {
            row: agent_session, 
            columns: [
            {link: agent_path(agent_session.agent), text: agent_session.agent.name},
            {text: agent_session.start_time},
            {text: agent_session.end_time},
            {text: agent_session.hostname},
            {text: agent_session.ros_master_uri},
            {text: agent_session.session_status},
            {text: agent_session.token}
            ]} %>
        <% end %>

<%= render partial: 'layouts/index_bottom' %>
