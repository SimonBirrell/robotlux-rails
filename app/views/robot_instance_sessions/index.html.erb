<%= render partial: 'layouts/index_top', locals: {items_name: 'robot_instance_sessions', headings: ['Name', 'Robot Instance', 'Start', 'End', 'Token']} %>

        <% @robot_instance_sessions.each do |robot_instance_session| %>
          <%= render partial: 'layouts/index_row', locals: {
            row: robot_instance, 
            columns: [
            {link: robot_instance_session_path(robot_instance_session), text: robot_instance_session.name},
            {link: robot_instance_path(robot_instance_session.robot_instance), text: robot_instance_session.robot_instance.name},
            {text: robot_instance.start_session},
            {text: robot_instance.end_session},
            {text: robot_instance.token}
            ]} %>
        <% end %>

<%= render partial: 'layouts/index_bottom' %>